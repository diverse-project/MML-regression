import pandas as pd 
from sklearn.model_selection import train_test_split 
from sklearn.model_selection import cross_val_score 
from sklearn.ensemble import GradientBoostingRegressor 
from sklearn.metrics import mean_absolute_error 
from sklearn.metrics import mean_squared_error 

df = pd.read_csv('/home/hugues/Documents/eclipse/runtime-EclipseXtext/testmml/src/Boston.csv')
X = df[['crim','zn','indus','chas',df.columns[8]]]
y = df['medv']

clf = GradientBoostingRegressor()
y_test = cross_val_score(clf, X, y, cv=50)
X_test = y

print 'Mean Squared Error: {}'.format(mean_absolute_error(y_test, clf.predict(X_test)))
print 'Mean Squared Error: {}'.format(mean_squared_error(y_test, clf.predict(X_test)))
